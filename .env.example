# FuelFlow Environment Variables
# Copy this file to .env and fill in your actual values

# App Environment
NODE_ENV=development

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:web:abcdef123456
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY_DEV=pk_test_your_dev_key_here
STRIPE_PUBLISHABLE_KEY_PROD=pk_live_your_prod_key_here
STRIPE_SECRET_KEY_DEV=sk_test_your_dev_secret_here
STRIPE_SECRET_KEY_PROD=sk_live_your_prod_secret_here

# Google Maps API Keys
GOOGLE_MAPS_ANDROID_API_KEY=your_android_maps_key_here
GOOGLE_MAPS_IOS_API_KEY=your_ios_maps_key_here
GOOGLE_MAPS_WEB_API_KEY=your_web_maps_key_here

# External APIs
GASBUDDY_API_KEY=your_gasbuddy_api_key_here
AAA_FUEL_API_KEY=your_aaa_fuel_api_key_here

# Notification Services
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890
SENDGRID_API_KEY=your_sendgrid_api_key

# Development URLs
API_BASE_URL=http://localhost:3000/api
WEB_BASE_URL=http://localhost:3000

# Production URLs (for production builds)
API_BASE_URL_PROD=https://api.fuelflow.com
WEB_BASE_URL_PROD=https://fuelflow.com

# Demo Configuration
DEMO_MODE=false
DEMO_USER_EMAIL=demo@fuelflow.com
DEMO_USER_PASSWORD=demo123456

# Analytics
GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
FIREBASE_ANALYTICS_ENABLED=true

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_DARK_MODE=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_PRICE_ALERTS=true
ENABLE_RECURRING_ORDERS=true

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Database (if using additional databases)
DATABASE_URL=postgresql://user:password@localhost:5432/fuelflow
REDIS_URL=redis://localhost:6379

# CI/CD
CODECOV_TOKEN=your_codecov_token_here
SNYK_TOKEN=your_snyk_token_here

# App Store Configuration
APP_STORE_CONNECT_API_KEY=your_app_store_connect_api_key
APP_STORE_CONNECT_ISSUER_ID=your_issuer_id
APP_STORE_CONNECT_KEY_ID=your_key_id

# Google Play Console
GOOGLE_PLAY_SERVICE_ACCOUNT=path_to_service_account.json

# Slack Notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url

# Expo Configuration
EXPO_TOKEN=your_expo_token_here
EAS_PROJECT_ID=your_eas_project_id

# Vercel Configuration
VERCEL_TOKEN=your_vercel_token_here